{
  "version": 2,
  "builds": [
    {
      "src": "front-react/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/front-react/$1"
    }
  ],
  "env": {
    "MONGO_URL": "@mongo_url"
  }
} 